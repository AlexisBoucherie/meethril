{% extends 'base.html.twig' %}

{% block title %}UserSession index{% endblock %}

{% block body %}
    <h1>UserSession index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>UserIsOwner</th>
                <th>PcName</th>
                <th>PcRace</th>
                <th>PcClass</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_session in user_sessions %}
            <tr>
                <td>{{ user_session.id }}</td>
                <td>{{ user_session.userIsOwner ? 'Yes' : 'No' }}</td>
                <td>{{ user_session.pcName }}</td>
                <td>{{ user_session.pcRace }}</td>
                <td>{{ user_session.pcClass }}</td>
                <td>{{ user_session.createdAt ? user_session.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user_session.updatedAt ? user_session.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_user_session_show', {'id': user_session.id}) }}">show</a>
                    <a href="{{ path('app_user_session_edit', {'id': user_session.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_session_new') }}">Create new</a>
{% endblock %}
